<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.act.quzhibo.view.TitleBarView
        android:id="@+id/titlebar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/commonViewHeight" />

    <com.act.quzhibo.view.ExpanbleCommonView
        android:id="@+id/expand_commonview"
        android:layout_width="match_parent"
        android:listSelector="@color/colorbg"
        android:layout_height="match_parent"
        android:layout_marginBottom="80dp"
        android:layout_marginTop="@dimen/commonViewHeight" />

    <Button
        android:id="@+id/moneyway_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center"
        android:layout_marginBottom="15dp"
        android:background="@drawable/button_common_selector"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:textSize="15sp"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:text="你还未选择财富通道" />
</FrameLayout>